<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<SelectDialog
		id="selectDialogTicket"
		title="Select Ticket ID"
		items="{P>/}"
		search=".onValueHelpSearchTicket"
		confirm=".onValueHelpCloseTicket"
		cancel=".onValueHelpCloseTicket">
		<StandardListItem
			icon="{ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{P>TicketID}"
			description="{P>TicketID}" />
	</SelectDialog>
</core:FragmentDefinition>

